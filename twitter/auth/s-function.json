{
  "name": "auth",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: serverless-auth",
  "customName": false,
  "customRole": false,
  "handler": "auth/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "twitter/auth",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "app_id": "$input.params('app_id')",
          "host": "$input.params().header.get('Host')",
          "stage": "$context.stage"
        }
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "301",
          "responseParameters": {
            "method.response.header.Location": "integration.response.body.redirectUrl",
            "method.response.header.Set-Cookie": "integration.response.body.setCookie"
          },
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
